<?xml version="1.0" encoding="utf-8"?>
<rx:Application 
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:rx="http://rx.reflex.io/2010">
	
	<!--
	In Reflex, component behavior is not baked into the code.
	User interactions are defined by reassignable "Behavior" classes.
	-->
	<rx:Button x="100" y="100">
		<rx:behaviors>
			<rx:ButtonBehavior />
			<rx:DragBehavior />
		</rx:behaviors>
	</rx:Button>
	
</rx:Application>
